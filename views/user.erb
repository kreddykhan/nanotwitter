<h3> <%= @user.username %> </h3>

<ul>
<% @user.followed.each do |follower| %>
  <li>
    <%= follower.username %>
  </li>
<% end %>
</ul>

<ul>
<% @tweets = Tweet.where(user_id: @user.id)%>
<% @tweets.each do |tweet| %>
  <li>
    <%= tweet.body %>
  </li>
<% end %>
</ul>

